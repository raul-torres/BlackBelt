@model LoginUser

<form action="login" method="post">
    <h1 class="text-center">Login</h1>
    <br>
    <div class="form-group">
        <label>Email</label>
            <span class="error1" asp-validation-for="LEmail"></span>
        <input placeholder="Email" class="form-control" asp-for="LEmail" value="raul@mail.com">
    </div>
    <div class="form-group">
        <label>Password</label>
        <span class="error1" asp-validation-for="LPassword"></span>
        <input placeholder="Password" class="form-control" asp-for="LPassword" value="q@123456789">
    </div>
    <div class="text-center">
        <input type="submit" value="Login" class="btn btn-primary"> 
    </div>
</form>